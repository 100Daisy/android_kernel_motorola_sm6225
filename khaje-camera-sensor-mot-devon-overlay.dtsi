		};

		config {
			pins = "gpio54";
			bias-pull-down; /* PULL DOWN */
			drive-strength = <2>; /* 2 MA */
			output-low;
		};
	};

	cam_main_dvdd_active: cam_main_dvdd_active {
		/* MAIN DVDD EN */
