		};
	};

	cam_front_dvdd_active: cam_front_dvdd_active {
		/* FRONT DVDD EN */
		mux {
			pins = "gpio60";
			function = "gpio";
		};

		config {
			pins = "gpio60";
			bias-disable; /* No PULL */
			drive-strength = <2>; /* 2 MA */
		};
	};

	cam_front_dvdd_suspend: cam_front_dvdd_suspend {
