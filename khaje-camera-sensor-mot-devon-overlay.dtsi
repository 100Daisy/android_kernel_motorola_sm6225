	};

	cam_main_afvdd_active: cam_main_afvdd_active {
		/* MAIN AFVDD EN */
		mux {
			pins = "gpio112";
			function = "gpio";
		};

		config {
			pins = "gpio112";
			bias-disable; /* No PULL */
			drive-strength = <2>; /* 2 MA */
		};
	};

	cam_main_afvdd_suspend: cam_main_afvdd_suspend {
		mux {
			pins = "gpio112";
			function = "gpio";
		};

		config {
			pins = "gpio112";
			bias-pull-down; /* PULL DOWN */
